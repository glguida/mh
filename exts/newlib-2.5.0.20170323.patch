+/*
+ * Find Last Set functions
+ */
+#ifndef fls32
+static __inline int __unused
+fls32(uint32_t _n)
+{
+	int _v;
+
+	if (!_n)
+		return 0;
+
+	_v = 32;
+	if ((_n & 0xFFFF0000U) == 0) {
+		_n <<= 16;
+		_v -= 16;
+	}
+	if ((_n & 0xFF000000U) == 0) {
+		_n <<= 8;
+		_v -= 8;
+	}
+	if ((_n & 0xF0000000U) == 0) {
+		_n <<= 4;
+		_v -= 4;
+	}
+	if ((_n & 0xC0000000U) == 0) {
+		_n <<= 2;
+		_v -= 2;
+	}
+	if ((_n & 0x80000000U) == 0) {
+		_n <<= 1;
+		_v -= 1;
+	}
+	return _v;
