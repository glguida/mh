+	rbt->rbt_insertions = 0;
+	rbt->rbt_removals = 0;
+	rbt->rbt_insertion_rebalance_calls = 0;
+	rbt->rbt_insertion_rebalance_passes = 0;
+	rbt->rbt_removal_rebalance_calls = 0;
+	rbt->rbt_removal_rebalance_passes = 0;
